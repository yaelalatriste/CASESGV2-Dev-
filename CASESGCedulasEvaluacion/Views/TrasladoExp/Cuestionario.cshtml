@* Cuestionario Traslado Expedientes *@
@model CedulasEvaluacion.Entities.TrasladoExp.TrasladoExpedientes
@{
    ViewData["Title"] = "Cédula de Traslado y Maniobra de Documentos - Folio: " + Model.Folio;
}

<div class="container-fluid" id="limpieza">
    <a href="#" type='button' id="btn_facturas" class='btn-sm btn-facturas float-right mr-2 mb-3' data-toggle="tooltip" title="Ver/Agregar Facturas" data-placement="top"><i class="fal fa-file-invoice"></i></a>
    <a href="/trasladoExp/index" type='button' class='btn-sm btn-warning float-right mr-2 mb-3' data-toggle="tooltip" title="Regresar al Listado de Cédulas de Mensajería" data-placement="top"><i class="fal fa-arrow-left"></i></a>
    <div class="row col-lg-12 mt-2 mb-3">
        <table class="table">
            <tr>
                <td data-toggle="tooltip" title="Folio de evaluación asignado por el sistema" data-placement="top"><strong>Folio: </strong> @Model.Folio</td>
                <td data-toggle="tooltip" title="Año de evaluación del servicio" data-placement="top"><strong>Año: </strong>@Model.Anio</td>
                <td><strong data-toggle="tooltip" title="Mes evaluado" data-placement="top">Mes: </strong>@Model.Mes</td>
                <td data-toggle="tooltip" title="Estatus de la cédula del servicio de Limpieza" data-placement="top"><strong>Estatus: </strong>@Model.Estatus</td>
            </tr>
            <tr>
                <td id="total_facturas" data-toggle="tooltip" title='Cantidad de facturas asignadas a el mes de "@Model.Mes"' data-placement="top">
                    <strong>Facturas asignadas: </strong> @Model.facturas.Count
                </td>
                <td id="monto_facturas" data-toggle="tooltip" title="Monto total de las facturas capturadas en este folio" data-placement="top">
                    <strong>Monto Total IVA Incluido: </strong>@String.Format(System.Globalization.CultureInfo.CurrentCulture, "{0:C}", @Model.TotalMontoFactura)
                </td>
                @if (@Model.Levantamiento == 1 && @Model.Maniobras == 1)
                {
                    <td data-toggle="tooltip" title="Se requirio Levantamiento y Maniobras" data-placement="top"><strong>Servicio Requerido: </strong>Levantamiento de Inventario y Maniobras</td>
                }
                else if (@Model.Levantamiento == 1 && @Model.Maniobras == 0)
                {
                    <td data-toggle="tooltip" title="Se requirio Levantamiento de Inventario" data-placement="top"><strong>Servicio Requerido: </strong>Levantamiento de Inventario</td>
                }
                else if (@Model.Levantamiento == 0 && @Model.Maniobras == 1)
                {
                    <td data-toggle="tooltip" title="Se requirio Maniobras" data-placement="top"><strong>Servicio Requerido: </strong>Maniobras</td>
                }
                <td colspan="2"></td>
            </tr>
        </table>
    </div>
    <div class="card">
        <div class="card-header bg-joke text-white">
            <h5 class="mt-2">Evaluación de la cédula</h5>
        </div>
        <div class="card-body">
            <div class="row ">
                <div class="col-lg-12">
                    <ul class="nav nav-tabs" id="myTab" role="tablist">
                        <li class="nav-item" role="presentation">
                            <a class="nav-link active" id="profile-tab" data-toggle="tab" href="#pregunta_1" role="tab" aria-controls="profile" aria-selected="false">Pregunta 1</a>
                        </li>
                        <li class="nav-item" role="presentation">
                            <a class="nav-link" id="profile-tab" data-toggle="tab" href="#pregunta_2" role="tab" aria-controls="profile" aria-selected="false">Pregunta 2</a>
                        </li>
                        <li class="nav-item" role="presentation">
                            <a class="nav-link" id="contact-tab" data-toggle="tab" href="#pregunta_3" role="tab" aria-controls="contact" aria-selected="false">Pregunta 3</a>
                        </li>
                        <li class="nav-item" role="presentation">
                            <a class="nav-link" id="contact-tab" data-toggle="tab" href="#entregables" role="tab" aria-controls="contact" aria-selected="false">Entregables</a>
                        </li>
                    </ul>
                    <div class="tab-content" id="myTabContent">
                        @* Pregunta 1 *@
                        <div class="tab-pane fade mt-2 ml-3 show active" id="pregunta_1" role="tabpanel" aria-labelledby="home-tab">
                            <p class="text-black h5">1. ¿El servicio se prestó con el número de personal requerido?</p>
                            <div class="icheck-success d-inline">
                                <input type="radio" name="pregunta1" value="true" id="radioSuccess1" class="pregunta1">
                                <label for="radioSuccess1">Si</label>
                            </div>
                            <div class="icheck-success d-inline ml-3">
                                <input type="radio" name="pregunta1" value="false" id="radioSuccess2" class="pregunta1">
                                <label for="radioSuccess2">No</label>
                            </div>
                            <div class="form-row col-lg-12 mt-3" id="diviPregunta1">
                                <div class="form-row">
                                    <div class="col col-lg-1">
                                        <label for="peronsalSol" class="h6">Personal Solicitado</label>
                                        <input type="number" id="personalSolicitado" class="form-control">
                                    </div>
                                    <div class="col-lg-1">
                                        <label for="personalBrin" class="h6">Personal Brindado</label>
                                        <input type="number" id="personalBrindado" class="form-control">
                                    </div>
                                </div>
                            </div>
                        </div>
                        @* Fin de la Pregunta 1 *@
                        @* Pregunta 2 *@
                        <div class="tab-pane fade mt-2 ml-3" id="pregunta_2" role="tabpanel" aria-labelledby="profile-tab">
                            <p class="text-black h5">2. ¿Al servicio se presentó el personal debidamente uniformado e identificado?</p>
                            <div class="icheck-success d-inline">
                                <input type="radio" name="pregunta2" value="true" id="radioSuccess3" class="pregunta2">
                                <label for="radioSuccess3">Si</label>
                            </div>
                            <div class="icheck-success d-inline ml-3">
                                <input type="radio" name="pregunta2" value="false" id="radioSuccess4" class="pregunta2">
                                <label for="radioSuccess4">No</label>
                            </div>
                            <div class="form-row col-lg-12 mt-3" id="diviPregunta2">
                                <div class="form-row">
                                    <div class="col col-lg-6">
                                        <label for="peronsalSol" class="h6">Comentarios: </label>
                                        <textarea type="number" id="comentarios" class="form-control" style="resize:none"></textarea>
                                    </div>
                                </div>
                            </div>
                        </div>
                        @* Fin de la Pregunta 2 *@
                        @* Pregunta 3 *@
                        <div class="tab-pane fade mt-2 ml-3" id="pregunta_3" role="tabpanel" aria-labelledby="contact-tab">
                            <p class="text-black h5">3. ¿El personal se presentó con el equipo, maquinaría y herramientas necesarias para prestar el servicio?</p>
                            <div class="icheck-success d-inline">
                                <input type="radio" name="pregunta3" value="true" id="radioSuccess5" class="pregunta3">
                                <label for="radioSuccess5">Si</label>
                            </div>
                            <div class="icheck-success d-inline ml-3">
                                <input type="radio" name="pregunta3" value="false" id="radioSuccess6" class="pregunta3">
                                <label for="radioSuccess6">No</label>
                            </div>
                            <div class="row col-lg-12 mt-3" id="diviPregunta3">
                                <button class="btn btn-primary mt-2 mr-1 btn_addInci" data-pregunta="3">Registro de Incidencias</button>
                                <button class="btn btn-primary mt-2 btn_viewInci" data-pregunta="3">Consultar Incidencias</button>
                            </div>
                        </div>
                        @* Fin de la Pregunta 3 *@
                        @* Entregables *@
                        <div class="tab-pane fade mt-2 ml-3" id="entregables" role="tabpanel" aria-labelledby="contact-tab">
                            <div class="col-lg-12 mt-3">
                                <h3>Adjuntar Entregables</h3>
                                Adjuntar la Factura, el Acta Entrega - Recepción y la Validación del SAT.
                                <a href="#" type='button' class='btn-sm btn-success mr-2 float-right btn_addFile'><i class="fas fa-plus"></i></a>
                            </div>
                            <div class="row col-lg-12 mt-3">
                                <table class="table" id="tblEntregables">
                                    <thead>
                                        <tr>
                                            <th>Entregable</th>
                                            <th>Archivo</th>
                                            <th>Fecha de Carga</th>
                                            <th>Acciones</th>
                                        </tr>
                                    </thead>
                                    <tbody id="tEntregables"></tbody>
                                </table>
                            </div>
                        </div>
                        @* Fin de Entregables *@
                    </div>
                </div>
            </div>
        </div>
        <div class="card-footer">
            <div class=" col-lg-12">
                <button type="button" class="btn btn-primary float-right ml-3 btn_enviar" data-toggle="tooltip" title='Enviar la "Cédula" a evaluación' data-placement="top">Enviar a DAS</button>
                <button type="button" class="btn btn-success float-right btn_guardar" data-toggle="tooltip" title='Guardar la información capturada sin envíar la cédula a evaluación' data-placement="top">Guardar Cédula</button>
            </div>
        </div>
    </div>
</div>

@* Modal para agregar facturas la cedula *@
