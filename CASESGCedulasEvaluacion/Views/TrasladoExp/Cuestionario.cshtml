                         if ($('#customFile').val() == "") {
                             $('#customFile').addClass('is-invalid');
                             $('#error_customFile').css('display', 'block');

                        } else {
                             var fileName = $("#customFile").val().split("\\").pop();
                             var ext = fileName.split('.').pop();
                             if (ext != "pdf") {
                                 $('#customFile').addClass('is-invalid');
                                 $('#error_customFile').css('display', 'block');
                             } else {
                                 $('#customFile').removeClass('is-invalid');
                                 $('#customFile').addClass('is-valid');
                                 $('#error_customFile').css('display', 'none');
                             }
                        }
                     });

                    $('#comentarios_entregable').change(function () {
                        if ($('#comentarios_entregable').val() == "") {
                            $('#comentarios_entregable').addClass('is-invalid');
                            $('#error_comentariosEntre').css('display', 'block');

                            return false;
                        } else {
                            $('#comentarios_entregable').removeClass('is-invalid');
                            $('#comentarios_entregable').addClass('is-valid');
                            $('#error_comentariosEntre').css('display', 'none');
                        }
                    });
                /*FIN de Validaciones Entregables*/

            /************************************ FIN CARGA/EDICION ARCHIVOS ************************************/



            /******************************* Mostrar/Ocultar Campos de Días**********************************/
                $('.pregunta1').change(function () {
                    let pr = $(this).val();
                    if (pr == "false") {
                        $('#diviPregunta1').css('display', 'block');
                    } else {
                        $('#diviPregunta1').css('display', 'none');
                        $('#personalSolicitado').val('');
                        $('#personalBrindado').val('');
                    }
                });

                $('.pregunta2').change(function () {
                    let pr = $(this).val();
                    let fol = '@Model.Folio';
                    if (pr == "false") {
                        $('#diviPregunta2').css('display', 'block');
                    } else {
                        $('#diviPregunta2').css('display', 'none');
                        $('#comentarios').val('');
                    }
                });

                $('.pregunta3').change(function () {
                    let pr = $(this).val();
                    let fol = '@Model.Folio';
                    if (pr == "false") {
                        $('#diviPregunta3').css('display', 'block');
                    } else {
                        $('#diviPregunta3').css('display', 'none');
                    }
                });
            /**************************** FIN Mostrar/Ocultar Campos de Días *******************************/

            /*Errores en Adjuntar Archivos*/
                $('#error_tipoEntre').css('display', 'none');
                $('#error_customFile').css('display', 'none');
                $('#error_comentariosEntre').css('display', 'none');
            /*Fin Errores en Adjuntar Archivos*/

            /*Llenamos el Combo de Meses de Correspondencia*/
                let optionsMes = "<option value=''>Selecciona el Mes</option>";
                meses.forEach(function (mes) {
                    optionsMes += "<option value=" + mes + ">" + mes + "</option>"
                });
                $("#mes_correspondiente").html(optionsMes);
            /*Fin del Combo de inmuebles*/

            /************************************ GUARDAR/ENVIAR Cédula ************************************/
                /*Guardar la Cedula*/
                    $('.btn_guardar').click(function () {
                        var respuestas = new Array();
                        for (let i = 0; i < 3; i++) {
                            let respuesta = $("input[name=pregunta" + (i + 1) + "]:checked").val();
                            if (respuesta != null) {
                                if (i == 0 && respuesta == "false") {
                                    respuestas.push({
                                        CedulaTrasladoId: parseInt(model.id), Pregunta: (i + 1), Respuesta: (respuesta == "true"),Detalles: ($('#personalSolicitado').val() + "|" + $('#personalBrindado').val())
                                    });
                                } else if (i == 1 && respuesta == "false") {
                                    respuestas.push({ CedulaTrasladoId: parseInt(model.id), Pregunta: (i + 1), Respuesta: (respuesta == "true"), Detalles: $("# comentarios").val() });
                                } else {
                                    respuestas.push({ CedulaTrasladoId: parseInt(model.id), Pregunta: (i + 1), Respuesta: (respuesta == "true"), Detalles: null });
                                }
                            }
                        }

                        axios.post("/trasladoExp/evaluation", respuestas).then(response => {
                            if (response.status == 200) {
                                Swal.fire({
                                    'icon': "success",
                                    'title': 'Levantamiento y Traslado de Expedientes',
                                    'html': "Se guardaron las respuesta de la cédula correctamente."
                                });
                            } else {
                                Swal.fire({
                                    'icon': "error",
                                    'title': 'Levantamiento y Traslado de Expedientes',
                                    'html': "Hubo un error al guardar las respuestas de la cédula. Contacte al Administrador del Sistema"
                                });
                            }
                        });
                    });
                /*FIN de guardar la cedula*/

                /*Guardar la Cedula sin mensaje*/
                    function guardaCedula() {
                        var respuestas = new Array();
                        for (let i = 0; i < 6; i++) {
                            let respuesta = $("input[name=pregunta" + (i + 1) + "]:checked").val();
                            if (respuesta != null) {
                                if (respuesta == "true") {
                                    respuestas.push({ CedulaTrasladoId: parseInt(model.id), Pregunta: (i + 1), Respuesta: (respuesta == "true"), Detalles: null });
                                } else {
                                    let detalles = $('.detalles' + (i + 1)).val();
                                    respuestas.push({ CedulaTrasladoId: parseInt(model.id), Pregunta: (i + 1), Respuesta: (respuesta == "true"), Detalles: detalles });
                                }
                            }
                        }

                        axios.post("/trasladoExp/evaluation", respuestas).then(response => {
                            if (response.status == 200) {

                            } else {
                                Swal.fire({
                                    'icon': "error",
                                    'title': 'Levantamiento y Traslado de Expedientes',
                                    'html': "Hubo un error al guardar las respuestas de la cédula. Contacte al Administrador del Sistema"
                                });
                            }
                        });
                    }
                /*FIN de guardar la cedula*/

                /*Validamos que ya fueron adjuntados todos los entregables*/
                    function validaEntregables() {
                        var msgEntregables = ["Acta Entrega - Recepción", "Factura", "Validación del SAT"];
                        var entregables = ["ActaER", "Factura", "SAT"];
                        var msg = "";
                        var nota = false;
                        var found = false;
                        axios.get('/trasladoExp/getListadoEntregables/' + model.id).then(response => {
                            for (var j = 0; j < entregables.length; j++) {
                                for (var i = 0; i < response.data.length; i++) {
                                    if (entregables[j] == response.data[i].tipo) {
                                        found = true;
                                        break;
                                    } else {
                                        found = false;
                                    }
                                }
                                if (!found) {
                                    msg += "<strong> - "+msgEntregables[j]+"<br/></strong>";
                                }
                            }

                            if (msg != "") {
                                Swal.fire({
                                    'icon': 'error',
                                    'title': 'Levantamiento y Traslado de Expedientes',
                                    'html': 'Para envíar la cédula a evaluación, es necesario adjuntar los siguientes entregables: <br/><br/>' + msg
                                });
                                return false;
                            } else {
                                axios.post("/trasladoExp/sendCedula/"+model.id).then(response => {
                                    if (response.status == 200) {
                                        Swal.fire({
                                            'icon': "success",
                                            'title': 'Levantamiento y Traslado de Expedientes',
                                            'html': "Se envíó la cédula para su revisión correctamente."
                                        }).then(function () {
                                            window.location.href = "/trasladoExp/index";
                                        });
                                    } else {
                                        Swal.fire({
                                            'icon': "error",
                                            'title': 'Levantamiento y Traslado de Expedientes',
                                            'html': "Hubo un error al guardar las respuestas de la cédula. Contacte al Administrador del Sistema"
                                        });
                                    }
                                });
                            }
                        });
                    }

                /*FIN Validamos que ya fueron adjuntados todos los entregables*/

                /*Enviar la Cedula*/
                    $('.btn_enviar').click(function () {

                        if (model.facturas.length == 0) {
                            Swal.fire({
                                'icon': 'error',
                                'title': 'Levantamiento y Traslado de Expedientes',
                                'html': 'No has registrado ninguna factura en la cédula'
                            });
                            return false;
                        }

                        guardaCedula();

                        for (let i = 0; i < 3; i++) {
                            let respuesta = $("input[name=pregunta" + (i + 1) + "]:checked").val();
                            if (respuesta == null || respuesta == "") {
                                Swal.fire({
                                    'icon': 'error',
                                    'title': 'Levantamiento y Traslado de Expedientes',
                                    'html': 'No ha respondido la <strong>Pregunta ' + (i + 1) + '</strong>'
                                });
                                return false;
                            } else if (respuesta == "false") {
                                axios.get('/trasladoExp/getIncidencias/' + model.id + '/' + (i+1)).then(res => {
                                    res = res.data;
                                    if (res == 0) {
                                        Swal.fire({
                                            'icon': 'error',
                                            'title': 'Levantamiento y Traslado de Expedientes',
                                            'html': 'No has capturado incidencias en la <strong>Pregunta ' + (i + 1) + '</strong>'
                                        });
                                        return false;
                                    }
                                });
                            }
                        }

                        validaEntregables();
                    });
                /*Enviar de guardar la cedula*/

            /********************************** FIN GUARDAR/ENVIAR CÉDULA **********************************/
        }
    </script>
