                                            <th>Comentarios</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @{ f = 0;}
                                        @foreach (var inci in @Model.incidencias)
                                        {
                                            if (inci.Pregunta.Equals(4 + ""))
                                            {
                                                f++; 
                                                <tr>
                                                    <td>@inci.Tipo</td>
                                                    <td>@inci.FechaProgramada.ToString("dd/MM/yyyy")</td>
                                                    <td>@inci.FechaRealizada.ToString("dd/MM/yyyy")</td>
                                                    <td>@inci.Comentarios.Split("|")[0]</td>
