@model CedulasEvaluacion.Entities.MFumigacion.CedulaFumigacion;
@{
    var title = TempData["Title"] as string;
    ViewData["Title"] = "Revisión de la Cédula del Servicio de Fumigación y Fauna Nociva: " + Model.Folio;
}

<div class="row col-lg-12">
    <div class="col-lg-12">
