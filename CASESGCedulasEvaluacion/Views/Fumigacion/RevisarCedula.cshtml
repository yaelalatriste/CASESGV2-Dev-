                                            <th>Comentarios</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var inci in @Model.incidencias)
                                        {
                                            if (inci.Pregunta.Equals(3 + ""))
                                            {
                                                <tr>
                                                    <td>@inci.Tipo</td>
                                                    <td>@inci.HoraProgramada</td>
