@model IEnumerable<CedulasEvaluacion.Entities.MCatalogoServicios.DashboardCS>
@{
    ViewData["Title"] = "Obligaciones del Prestador de Servicio - Servicios Generales";
}
<div class="container-fluid" id="Prrueba">
    <div class="row mt-3">
        @foreach (var ds in Model)
        {
            <div class="col-lg-3 col-6 font-weight-bold">
                <!-- small box -->
                <div class="small-box @ds.Fondo">
                    <div class="inner">
                        <h5>Servicio de @ds.Servicio</h5>
                        <p><h6 class="font-weight-bold">@ds.Total - Contrato(s)</h6></p>
                        <div class="icon">
                            @Html.Raw(@ds.Icono)<br />
                        </div>
                    </div>
                    <a href="/catalogo/detalleServicio/@ds.Id" class="small-box-footer">Ver detalles<i class="fas fa-arrow-circle-right ml-2"></i></a>
                </div>
            </div>
        }
    </div>
</div>
@section Scripts{
    <script>
        $(function () {
            $('[data-toggle="tooltip"]').tooltip();
        });
    </script>
}

