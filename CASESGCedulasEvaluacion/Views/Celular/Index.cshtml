                    <thead>
                        <tr>
                            <th>ID</th>
                            <th>Estatus</th>
                            <th>Servicio</th>
                            <th>Inmueble</th>
                            <th>Folio</th>
                            <th>Mes</th>
                            <th>Año</th>
