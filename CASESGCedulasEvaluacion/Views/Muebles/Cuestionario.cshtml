                            });
                        } else {
                            Swal.fire({
                                'icon': 'error',
                                'title': 'Servicio de Transportación de Bienes Muebles',
                                'html': 'No ha capturado el <strong>Personal Solicitado</strong> y el <strong>Personal Proporcionado</strong> en la <strong>Pregunta ' + (i + 1) + '</strong>'
                            });
                        }
                    } else if ((i == 4 || i == 1) && isContinue == false) {
                        Swal.fire({
                            'icon': 'error',
                            'title': 'Servicio de Transportación de Bienes Muebles',
                            'html': 'No ha capturado incidencias en la <strong>Pregunta ' + (i + 1) + '</strong>'
                        });
                    } else{
                        validaEntregables();
                    }
                });

                $('.btn_enviarDAS').click(function () {
                    let i = 0;
                    let respuesta = false;
                    for (i = respuestas.length - 1; i > 4; i--) {
                        respuesta = $('input:radio[name="pregunta' + respuestas[i].pregunta + '"]:checked').val();
                        if (respuesta == "" || respuesta == null) {
                            break;
                        } else if ($("#fecha_programadaP" + respuestas[i].pregunta).val() == "" && $("#fecha_entregaP" + respuestas[i].pregunta).val() == "" && respuesta == "true") {
                            break;
                        }
                    }

                    if (respuesta == "" || respuesta == null) {
                        Swal.fire({
                            'icon': 'error',
                            'title': 'Servicio de Transportación de Bienes Muebles',
                            'html': 'No ha contestado la <strong>Pregunta ' + (i + 1) + '</strong>'
                        });
                    } else if ($("#fecha_programadaP" + respuestas[i].pregunta).val() == "" && $("#fecha_entregaP" + respuestas[i].pregunta).val() == "") {
                        Swal.fire({
                            'icon': 'error',
                            'title': 'Servicio de Transportación de Bienes Muebles',
                            'html': 'No ha capturado la fecha de programada y la fecha de entrega del entregable descrito en la <strong>Pregunta ' + (i + 1) + '</strong>'
                        });
                    } else {
                        validaEntregables();
                    }
                    return false;
                });
                /*Enviar de guardar la cedula*/

            /********************************** FIN GUARDAR/ENVIAR CÉDULA **********************************/
        });
    </script>
