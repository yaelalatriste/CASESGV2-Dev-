                                        });
                                        $('#customFile').removeClass('is-valid');
                                        $('#comentarios_entregable').removeClass('is-valid');
                                        $('#tipo_entregable').removeClass('is-valid');
                                    }
                                    axios.get("/telConvencional/getEntregables/" + model.id).then(success => {
                                        if (success.data == null || success.data == "") {
                                            $('#tEntregables').html(success.data);
                                        }
                                        $('#tEntregables').html(success.data);
                                    });
                                }).catch(error => {
                                    Swal.fire({
                                        'icon': "error",
                                        'title': 'Servicio de Telefonía Convencional',
                                        'text': 'Se presento un error al intentar adjuntar el archivo.'
                                    });
                                });

                                document.getElementById('customFile').value = '';
                                $('#file_id').val("");
                                $(".custom-file-label").text("Seleccionar Archivo");
                                $('#tipo_entregable').val('');
                                $('#comentarios_entregable').val('');
                            } else {
                                axios.get('/telConvencional/buscaEntregable/' + model.id + '/' + tipo).then(exist => {
                                    if (exist.data == 0) {
                                        axios.post('/telConvencional/adjuntaEntregable', formData, {
                                            headers: { 'Content-Type': 'multipart/form-data' }

                                        }).then(response => {
                                            $('#cerrar_modal_sua').trigger('click');
                                            /*Validamos la pregunta a la que se realizo el anexo de evidencia*/
                                            if (response.status == 200) {
                                                Swal.fire({
                                                    'icon': "success",
                                                    'title': 'Servicio de Telefonía Convencional',
                                                    'text': 'Se adjuntó el archivo de forma correcta.'
                                                });
                                                $('#customFile').removeClass('is-valid');
                                                $('#comentarios_entregable').removeClass('is-valid');
                                                $('#tipo_entregable').removeClass('is-valid');
                                            }
                                            axios.get("/telConvencional/getEntregables/" + model.id).then(success => {
                                                if (success.data == null || success.data == "") {
                                                    $('#tEntregables').html(success.data);
                                                }
                                                $('#tEntregables').html(success.data);
                                            });
                                        }).catch(error => {
                                            Swal.fire({
                                                'icon': "error",
                                                'title': 'Servicio de Telefonía Convencional',
                                                'text': 'Se presento un error al intentar adjuntar el archivo.'
                                            });
                                        });

                                        document.getElementById('customFile').value = '';
                                        $('#file_id').val("");
                                        $(".custom-file-label").text("Seleccionar Archivo");
                                        $('#tipo_entregable').val('');
                                        $('#comentarios_entregable').val('');
                                    } else {
                                        Swal.fire({
                                            'icon': "error",
                                            'title': 'Servicio de Telefonía Convencional',
                                            'text': 'Ya fue enviado un entregable con el mismo tipo.'
                                        });
                                    }
                                }).catch(e => {
                                    Swal.fire({
                                        'icon': "error",
                                        'title': 'Servicio de Telefonía Convencional',
                                        'text': 'Ocurrío un error al válidar el entregable.'
                                    });
                                });
                            }
                        }
                    });

                /*FIN Adjunta Entregables*/

                /* Listado de Entregables de la Cedula */
                    axios.get("/telConvencional/getEntregables/"+model.id).then(response => {
                        if (response.data == null || response.data == "") {
                            $('#tEntregables').html(response.data);
                        }
                        $('#tEntregables').html(response.data);
                    });
                /* Fin Listado de Entregables de la Cedula*/

                /*Editar Entregables*/
                    $(document).on("click", ".update_files", function () {
                        $('#modal_evidencias').modal('show');
                        var id = $(this).data('id');
                        var tipo = $(this).data('tipo');
                        var coments = $(this).data('coments');
                        var file = $(this).data('file');
                        $('#file_id').val(parseInt(id));
                        $(".custom-file-label").text(file);
                        $('#tipo_entregable').val(tipo);
                        $('#comentarios_entregable').val(coments);
                        $('#mes_correspondiente').prop('disabled', true);
                        $('#modal_evidencias').modal('show');
                    });
                /*FIN de editar Entregables*/

                /*Elimina entregables*/
                   $(document).on("click", ".delete_files", function () {
                        let id = $(this).data('id');
                        let tipo = $(this).data('tipo');
                        Swal.fire({
                            'icon': 'warning',
                            'title': '¿Deseas Continuar?',
                            'text': '¿Estás seguro de eliminar el archivo relacionado a "'+tipo+'"?',
                            'confirmButtonColor': '#3085d6',
                            'cancelButtonColor': '#d33',
                            'confirmButtonText': 'Si, Eliminar Archivo',
                            'cancelButtonText': 'Cancelar',
                            'showCancelButton': true
                        }).then(response => {
                            if (response.isConfirmed) {
                                axios.post("/telConvencional/eliminaArchivo", { Id: id, Folio: model.folio }).then(response => {
                                    if (response.status == 200) {
                                        Swal.fire(
                                            'Servicio de Telefonía Convencional',
                                            'El archivo fue eliminado con éxito.',
                                            'success'
                                        );
                                        axios.get("/telConvencional/getEntregables/" + model.id).then(response => {
                                            if (response.data == null || response.data == "") {
                                                $('#tEntregables').html(response.data);
                                            }
                                            $('#tEntregables').html(response.data);
                                        });
                                    }
                                }).catch(error => {
                                    Swal.fire(
                                        'Servicio de Telefonía Convencional',
                                        'No se pudo procesar tu solicitud. Contacta a tu Administrador.',
                                        'error'
                                    );
                                });
                            }
                        });
                    });
                 /*FIN de elimina entregables*/

                /*Ver archivo en modal (REVISAR PARA AJUSTAR)*/
                    $(document).on('click', '.view_file', function () {
                        let nombre = $(this).data('file');
                        let tipo = $(this).data('tipo');
                        $('#titlePDF').text("PDF - \"" + tipo + "\"");
                        $('#objectPdf').attr('data', '/cedLimpieza/verArchivo/' + model.folio + '/' + nombre);
                        $('#modal_pdf').modal('show');
                    });
                /*FIN de ver archivo en modal*/

                /*Validaciones Entregables*/
                    $('#tipo_entregable').change(function () {
                        if ($('#tipo_entregable').val() == "") {
                            $('#tipo_entregable').addClass('is-invalid');
                            $('#error_tipoEntre').css('display', 'block');

                            return false;
                        } else {
                            $('#tipo_entregable').removeClass('is-invalid');
                            $('#tipo_entregable').addClass('is-valid');
                            $('#error_tipoEntre').css('display', 'none');
                        }
                    });

                    $('#customFile').change(function () {
                         if ($('#customFile').val() == "") {
                             $('#customFile').addClass('is-invalid');
                             $('#error_customFile').css('display', 'block');

                        } else {
                             var fileName = $("#customFile").val().split("\\").pop();
                             var ext = fileName.split('.').pop();
                             if (ext != "pdf") {
                                 $('#customFile').addClass('is-invalid');
                                 $('#error_customFile').css('display', 'block');
                             } else {
                                 $('#customFile').removeClass('is-invalid');
                                 $('#customFile').addClass('is-valid');
                                 $('#error_customFile').css('display', 'none');
                             }
                        }
                     });

                    $('#comentarios_entregable').change(function () {
                        if ($('#comentarios_entregable').val() == "") {
                            $('#comentarios_entregable').addClass('is-invalid');
                            $('#error_comentariosEntre').css('display', 'block');

                            return false;
                        } else {
                            $('#comentarios_entregable').removeClass('is-invalid');
                            $('#comentarios_entregable').addClass('is-valid');
                            $('#error_comentariosEntre').css('display', 'none');
                        }
                    });
                /*FIN de Validaciones Entregables*/

            /************************************ FIN CARGA/EDICION ARCHIVOS ************************************/

            /************************************ GUARDAR/ENVIAR Cédula ************************************/
                /*Guardar la Cedula*/
                    $('.btn_guardar').click(function () {
                        var respuestas = new Array();
                        for (let i = 0; i < 11; i++) {
                            let respuesta = $("input[name=pregunta" + (i + 1) + "]:checked").val();
                            if (respuesta != null) {
                                respuestas.push({ CedulaEvaluacionId: parseInt(model.id), Pregunta: (i + 1), Respuesta: (respuesta == "true") });
                            }
                        }

                        axios.post("/telConvencional/evaluation", respuestas).then(response => {
                            if (response.status == 200) {
                                Swal.fire({
                                    'icon': "success",
                                    'title': "Servicio de Telefonía Convencional",
                                    'html': "Se guardaron las respuesta de la cédula correctamente."
                                });
                            } else {
                                Swal.fire({
                                    'icon': "error",
                                    'title': "Servicio de Telefonía Convencional",
                                    'html': "Hubo un error al guardar las respuestas de la cédula. Contacte al Administrador del Sistema"
                                });
                            }
                        });
                    });
                /*FIN de guardar la cedula*/

                /*Guardar la Cedula sin mensaje*/
                    function guardaCedula() {
                        var respuestas = new Array();
                        for (let i = 0; i < 11; i++) {
                            let respuesta = $("input[name=pregunta" + (i + 1) + "]:checked").val();
                            if (respuesta != null) {
                                respuestas.push({ CedulaEvaluacionId: parseInt(model.id), Pregunta: (i + 1), Respuesta: (respuesta == "true") });
                            }
                        }

                        axios.post("/telConvencional/evaluation", respuestas).then(response => {
                            if (response.status == 200) {

                            } else {
                                Swal.fire({
                                    'icon': "error",
                                    'title': "Servicio de Telefonía Convencional",
                                    'html': "Hubo un error al guardar las respuestas de la cédula. Contacte al Administrador del Sistema"
                                });
                            }
                        });
                    }
                /*FIN de guardar la cedula*/

                /*Validamos que ya fueron adjuntados todos los entregables*/
                    function validaEntregables() {
                        var msgEntregables = ["Acta Entrega - Recepción", "Factura", "Validación del SAT", "Reporte de Servicio","Cuadro de Servicios"];
                        var entregables = ["ActaER", "Factura", "SAT", "Reporte de Servicio", "Cuadro de Servicios"];
                        var msg = "";
                        var nota = false;
                        var found = false;
                        axios.get('/telConvencional/getListadoEntregables/' + model.id).then(response => {
                            for (var j = 0; j < entregables.length; j++) {
                                for (var i = 0; i < response.data.length; i++) {
                                    if (entregables[j] == response.data[i].tipo) {
                                        found = true;
                                        break;
                                    } else {
                                        found = false;
                                    }
                                }
                                if (!found) {
                                    msg += "<strong> - "+msgEntregables[j]+"<br/></strong>";
                                }
                            }


                            if (msg != "") {
                                Swal.fire({
                                    'icon': 'error',
                                    'title': "Servicio de Telefonía Convencional",
                                    'html': 'Para envíar la cédula a evaluación, es necesario adjuntar los siguientes entregables: <br/><br/>' + msg
                                });
                                return false;
                            } else {
                                axios.post('/telConvencional/historialConvencional', {
                                    ServicioId: model.servicioId,
                                    CedulaId: model.id, UsuarioId: parseInt(@User.Claims.ElementAt(0).Value),
                                    Estatus: "Enviado a DAS", Comentarios: "Se envía la cédula a la DAS para su revisión."
                                }).then(response => {
                                    axios.post("/telConvencional/sendCedula/" + model.servicioId + "/" + model.id).then(response => {
                                        if (response.status == 200) {
                                            Swal.fire({
                                                'icon': "success",
                                                'title': "Servicio de Telefonía Convencional",
                                                'html': "Se envíó la cédula para su revisión correctamente."
                                            }).then(function () {
                                                window.location.href = "/telConvencional/index/" + model.servicioId;
                                            });
                                        } else {
                                            Swal.fire({
                                                'icon': "error",
                                                'title': "Servicio de Telefonía Convencional",
                                                'html': "Hubo un error al guardar las respuestas de la cédula. Contacte al Administrador del Sistema"
                                            });
                                        }
                                    });
                                });
                            }
                        });
                    }

                /*FIN Validamos que ya fueron adjuntados todos los entregables*/

                /*Enviar la Cedula*/
                    $('.btn_enviar').click(function () {
                        if (model.facturas.length == 0) {
                            Swal.fire({
                                'icon': 'error',
                                'title': 'Servicio de Telefonía Celular',
                                'html': 'No has registrado ninguna factura en la cédula'
                            });
                            return false;
                        }
                        guardaCedula();

                        for (let i = 0; i < 11; i++) {
                            let respuesta = $("input[name=pregunta" + (i + 1) + "]:checked").val();
                            if (respuesta == null || respuesta == "") {
                                Swal.fire({
                                    'icon': 'error',
                                    'title': 'Servicio de Telefonía Convencional',
                                    'html': 'No ha respondido la <strong>Pregunta ' + (i + 1) + '</strong>'
                                });
                                return false;
                            }else if (respuesta == 'true') {
                                axios.get('/telConvencional/totalIncidencia/' + model.id + '/' + tiposIncidencias[i]).then(response => {
                                    response = response.data;
                                    if (parseInt(response) == 0) {
                                        Swal.fire({
                                            'icon': 'error',
                                            'title': 'Servicio de Telefonía Convencional',
                                            'html': 'No ha registrado ningún requerimiento en la <strong>Pregunta ' + (i + 1) + '</strong>.'
                                        });
                                        return false;
                                    }
                                });
                            }
                        }
                        validaEntregables();
                    });
                /*Enviar de guardar la cedula*/

            /********************************** FIN GUARDAR/ENVIAR CÉDULA **********************************/
        }
    </script>
}

