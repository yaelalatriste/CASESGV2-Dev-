                             var fileName = $("#customFile").val().split("\\").pop();
                             var ext = fileName.split('.').pop();
                             if (ext != "pdf") {
                                 $('#customFile').addClass('is-invalid');
                                 $('#error_customFile').css('display', 'block');
                             } else {
                                 $('#customFile').removeClass('is-invalid');
                                 $('#customFile').addClass('is-valid');
                                 $('#error_customFile').css('display', 'none');
                             }
                        }
                     });

                    $('#comentarios_entregable').change(function () {
                        if ($('#comentarios_entregable').val() == "") {
                            $('#comentarios_entregable').addClass('is-invalid');
                            $('#error_comentariosEntre').css('display', 'block');

                            return false;
                        } else {
                            $('#comentarios_entregable').removeClass('is-invalid');
                            $('#comentarios_entregable').addClass('is-valid');
                            $('#error_comentariosEntre').css('display', 'none');
                        }
                    });
                /*FIN de Validaciones Entregables*/

            /************************************ FIN CARGA/EDICION ARCHIVOS ************************************/

            /************************************ GUARDAR/ENVIAR Cédula ************************************/
                /*Guardar la Cedula*/
                    $('.btn_guardar').click(function () {
                        var respuestas = new Array();
                        var detalles = "";
                        for (let i = 0; i < 6; i++) {
                            let respuesta = $("input[name=pregunta" + (i + 1) + "]:checked").val();
                            if (respuesta != null) {
                                if (respuesta == "true") {
                                    respuestas.push({ CedulaEvaluacionId: parseInt(model.id), Pregunta: (i + 1), Respuesta: (respuesta == "true"), Detalles: null });
                                } else {
                                    if (i == 0) {
                                        detalles = $("#fecha_programada").val() + "|" + $("#fecha_realizada").val();
                                    } else if ( i == 2 ) {
                                        detalles = $("#comentarios_pregunta3").val();
                                    }
                                    respuestas.push({ CedulaEvaluacionId: parseInt(model.id), Pregunta: (i + 1), Respuesta: (respuesta == "true"), Detalles: detalles });
                                }
                            }
                            detalles = null;
                        }

                        axios.post("/residuos/evaluation", respuestas).then(response => {
                            if (response.status == 200) {
                                Swal.fire({
                                    'icon': "success",
                                    'title': 'Servicio de RPBI',
                                    'html': "Se guardaron las respuesta de la cédula correctamente."
                                });
                            } else {
                                Swal.fire({
                                    'icon': "error",
                                    'title': 'Servicio de RPBI',
                                    'html': "Hubo un error al guardar las respuestas de la cédula. Contacte al Administrador del Sistema"
                                });
                            }
                        });
                    });
                /*FIN de guardar la cedula*/

                /*Guardar la Cedula sin mensaje*/
                    function guardaCedula() {
                        var respuestas = new Array();
                        var detalles = "";
                        for (let i = 0; i < 6; i++) {
                            let respuesta = $("input[name=pregunta" + (i + 1) + "]:checked").val();
                            if (respuesta != null) {
                                if (respuesta == "true") {
                                    respuestas.push({ CedulaEvaluacionId: parseInt(model.id), Pregunta: (i + 1), Respuesta: (respuesta == "true"), Detalles: null });
                                } else {
                                    if (i == 0) {
                                        detalles = $("#fecha_programada").val() + "|" + $("#fecha_realizada").val();
                                    } else if (i == 2) {
                                        detalles = $("#comentarios_pregunta3").val();
                                    }
                                    respuestas.push({ CedulaEvaluacionId: parseInt(model.id), Pregunta: (i + 1), Respuesta: (respuesta == "true"), Detalles: detalles });
                                }
                            }
                            detalles = null;
                        }

                        axios.post("/residuos/evaluation", respuestas).then(response => {
                            if (response.status == 200) {

                            } else {
                                Swal.fire({
                                    'icon': "error",
                                    'title': 'Servicio de RPBI',
                                    'html': "Hubo un error al guardar las respuestas de la cédula. Contacte al Administrador del Sistema"
                                });
                            }
                        });
                    }
                /*FIN de guardar la cedula*/

                /*Validamos que ya fueron adjuntados todos los entregables*/
                    function validaEntregables() {
                        var msgEntregables = ["Acta Entrega - Recepción", "Factura", "Validación del SAT","Manifiesto"];
                        var entregables = ["ActaER", "Factura", "SAT","Manifiesto"];
                        var msg = "";
                        var nota = false;
                        var found = false;
                        axios.get('/residuos/getListadoEntregables/' + model.id).then(response => {
                            for (var j = 0; j < entregables.length; j++) {
                                for (var i = 0; i < response.data.length; i++) {
                                    if (entregables[j] == response.data[i].tipo) {
                                        found = true;
                                        break;
                                    } else {
                                        found = false;
                                    }
                                }
                                if (!found) {
                                    msg += "<strong> - "+msgEntregables[j]+"<br/></strong>";
                                }
                            }


                            if (msg != "") {
                                Swal.fire({
                                    'icon': 'error',
                                    'title': 'Servicio de RPBI',
                                    'html': 'Para envíar la cédula a evaluación, es necesario adjuntar los siguientes entregables: <br/><br/>' + msg
                                });
                                return false;
                            } else {
                                axios.post("/residuos/sendCedula/" + model.servicioId + "/" + model.id).then(response => {
                                    if (response.status == 200) {
                                        Swal.fire({
                                            'icon': "success",
                                            'title': 'Servicio de RPBI',
                                            'html': "Se envíó la cédula para su revisión correctamente."
                                        }).then(function () {
                                            window.location.href = "/residuos/index";
                                        });
                                    } else {
                                        Swal.fire({
                                            'icon': "error",
                                            'title': 'Servicio de RPBI',
                                            'html': "Hubo un error al guardar las respuestas de la cédula. Contacte al Administrador del Sistema"
                                        });
                                    }
                                });
                            }
                        });
                    }

                /*FIN Validamos que ya fueron adjuntados todos los entregables*/



                /********************************** Funciones Adicionales *********************************/
                    function cuentaIncidenciasRPBI(pregunta) {
                        var isContinue = true;
                        if (pregunta == 4) {
                            $.ajax({
                                url: '/residuos/getIncidenciasPregunta4/' + model.id + '/ManifiestoEntrega',
                                type: 'GET',
                                async: false,
                                success: function (json) {
                                    json == 0 ? isContinue = false : true;
                                },
                            });
                        } else {
                            $.ajax({
                                url: '/residuos/getIncidenciasPregunta5/' + model.id,
                                type: 'GET',
                                async: false,
                                success: function (json) {
                                    json == 0 ? isContinue = false : true;
                                },
                            });
                        }
                        return isContinue;
                    }
            /********************************** FIN Funciones Asincronas*********************************/


                /*Enviar la Cedula*/
                $('.btn_enviar').click(function () {
                    var next = true;
                    let respuesta = true;
                    let i = 0;
                    if (model.facturas.length == 0) {
                        Swal.fire({
                            'icon': 'error',
                            'title': 'Servicio de RPBI',
                            'html': 'No has registrado ninguna factura en la cédula'
                        });
                        return false;
                    }
                    guardaCedula();

                    for (i = 0; i < 5; i++) {
                        respuesta = $("input[name=pregunta" + (i + 1) + "]:checked").val();
                        next = true;
                        if (respuesta == null || respuesta == "") {
                            next = false;
                            break;
                        }
                        else if (i == 2 && respuesta == 'false' && $("#comentarios_pregunta3").val() == "") {
                            next = false;
                            break;
                        }
                        else if (i == 3 && respuesta == 'false') {
                            next = cuentaIncidenciasRPBI((i+1));
                            if (next == false) {
                                break;
                            }
                        }
                        else if (i == 4 && respuesta == 'false') {
                            next = cuentaIncidenciasRPBI((i + 1));
                            if (next == false) {
                                break;
                            }
                        }
                    }

                    if(respuesta == null || respuesta ==""){
                        Swal.fire({
                            'icon': 'error',
                            'title': 'Servicio de RPBI',
                            'html': 'No ha respondido la <strong>Pregunta ' + (i + 1) + '</strong>'
                        });
                        return false;
                    }
                    else if (i == 2 && respuesta == 'false') {
                        Swal.fire({
                            'icon': 'error',
                            'title': 'Servicio de RPBI',
                            'html': 'Por favor capture los comentarios en la <b>Pregunta '+(i+1)+'</b>.'
                        });
                        return false;
                    }
                    else if (i == 3 && respuesta == 'false') {
                        Swal.fire({
                            'icon': 'error',
                            'title': 'Servicio de RPBI',
                            'html': 'Ya se respondió la <strong>Pregunta ' + (i + 1) + ' </strong> pero no se han capturado <strong>incidencias</strong>'
                        });
                        return false;
                    } else if (i == 4 && respuesta == 'false') {
                        Swal.fire({
                            'icon': 'error',
                            'title': 'Servicio de RPBI',
                            'html': 'Ya se respondió la <strong>Pregunta ' + (i + 1) + '</strong> pero no se han capturado <strong>incidencias</strong>.'
                        });
                        return false;
                    }
                    validaEntregables();
                });
                /*Enviar de guardar la cedula*/

            /********************************** FIN GUARDAR/ENVIAR CÉDULA **********************************/
        }
    </script>
